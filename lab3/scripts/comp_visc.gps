set term pdfcairo
set output 'visc_273-15.pdf'

set xlabel 'Density (kg/m^3)' font "times,16"
set ylabel 'Viscosity (10^{-5} Pa*s)' font "times,16"
set key left top box

plot 'sim_data.dat' u 1:3 with lines linewidth 2 title 'Our TraPPE',\
     'TraPPE_273.15K.dat' u 1:3 w li lw 2 title 'Lit. TraPPE',\
     'OPLS_273.15K.dat' u 1:3 w li lw 2 ti 'Lit. OPLS',\
     'NIST_273.15K.dat' u 1:3 w li lw 2 ti 'NIST'
